<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
  <link rel="stylesheet" href="/css/main.css">
</head>
<body>
  <h1>Users page</h1>
  <% if (users.length > 0) { %>
    <ul>
      <% for (let user of users) { %>
        <li><%= user.user %></li>
      <% } %>
    </ul>
  <% } else { %>
    <span>No users found</span>
  <% } %>
</body>
</html>